<%- include ('templates/header'); -%>
<h3>Add New Author</h3>
	<form action='ticket' method='POST'>
		<div class='form-group row'>
			<label for='userID' class='col-sm-4 col-form-label'>userID</label>
			<div class='col-sm-4'>
				<input type='text' class='form-control' id='firstname' name='userID' placeholder='Jane' required>
			</div>
		</div>
		<div class='form-group row'>
			<label for='broken Item' class='col-sm-4 col-form-label'>broken Item</label>
			<div class='col-sm-4'>
				<input type='text' class='form-control' id='lastname' name='brokenItem' placeholder='Doe' required>
			</div>
		</div>
		</div>
		<div class='form-group row'>
			<label for='problem' class='col-sm-4 col-form-label'>problem </label>
			<div class='col-sm-4'>
				<textarea class='form-control' rows='5' id='biography' name='problem'></textarea>
			</div>
		</div>
		<div class='form-group row'>
			<div class='col-sm-4'></div>
			<div class='col-sm-4'>
				<button class='btn btn-primary btn-block'>Add ticket</button>
				
			</div>
			<div class='col-sm-4'></div>
		</div>
	</form>
<button href= "/" class='btn btn-danger btn-lrg'>Home</button>
</div>